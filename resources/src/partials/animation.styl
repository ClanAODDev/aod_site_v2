
@keyframes fade-in{
    0%{
        opacity:0
    }
    to{
        opacity:1
    }
}

@keyframes drop-in-fade-in{
    0%{
        opacity:0;
        transform:translateY(-20px)
    }
    to{
        opacity:1;
        transform:translateY(0)
    }
}

